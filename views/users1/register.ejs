<!-- views/users1/register.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"></meta>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"></meta>
    <meta name="viewport" content="width=device-width, initial-scale=1"></meta>
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content=""></meta>
    <meta name="author" content=""></meta>
    <script src="/jquery/jquery.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/bootstrap/css/bootstrap.min.css"></link>
    <link rel="stylesheet" type="text/css" href="/css/footer.css"></link>
    <link rel="stylesheet" type="text/css" href="/css/login.css"></link>

    <title>Sticky Footer Navbar Template for Bootstrap</title>
</head>

<body>


<!-- Begin page content -->
<div class="container">
    <div id="wrapper">
        <div id="box">
            <div id="top_header">
                <h3>Register</h3>
                <h5>Register and receive OTP</h5>
            </div>

            <div id="inputs">
                <form id='login' action='/users1/register' method='post' accept-charset='UTF-8' style="margin-left:6px;">

                    <input type='hidden' name='submitted' id='submitted' value='1'/>

                    <div class=''>
                        <input type='tel' name='mobile' id='mobile' value='' maxlength="50" size="30" placeholder="10 digit Mobile"/><br/>
                    </div>

                    <div class=''>
                        <input type='email' name='email' id='email' value='' maxlength="50" size="30" placeholder="Email"/><br/>
                    </div>

                    <div class=''>
                        <input type='password' name='password' id='password' maxlength="50" size="30" placeholder="Password" /><br/>
                    </div>

                    <div class=''>
                        <input type='submit' name='Submit' value='Register' />
                    </div>

                </form>

                <div id="bottom">
                    <a href="/users1/login">Sign in</a>
                    <a class="right_a" href="#">Terms and Conditions</a>
                </div>
            </div>
        </div>
    </div>
</div>


</body>
</html>
