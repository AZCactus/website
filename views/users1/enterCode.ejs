<div class="container">
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center login-title">Enter the code
        <p><%= phone %></p>

        <div class="account-wall"><img src="" class="profile-img"/>
          <form action=<%= "/users1/enterCode?phone="+phone %> method="POST" class="form-signin">
            <input type="text" name="code" placeholder="" required="required" autofocus="autofocus" class="form-control"/>
            <button type="submit" class="btn btn-lg btn-primary btn-block">Verify Code</button><span class="clearfix"></span>
          </form>
        </div><a href="/users1/resendCode" class="text-center new-account">Resend Code </a>
        <div id="message"></div>
      </h1>
    </div>
  </div>
</div>